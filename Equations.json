                "acceleration":"a"
            }
        }
    },
    "gravitation":{
        "equations":{
            "force":"((6.67*pow(10,-11))*m1*m2) / (r*r)",
            "mass1":"(f*(r*r)) / (m2*(6.67*pow(10,-11)))",
            "mass2":"(f*(r*r)) / (m1*(6.67*pow(10,-11)))",
            "radius":"math.sqrt(((6.67*pow(10,-11))*m1*m2) / f)"
        },
        "variables":{
            "necessary":3,
            "type":{
                "force":"f",
                "mass1":"m1",
                "mass2":"m2",
                "radius":"r"
            }
        }
    },
    "circular":{
